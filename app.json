{
  "expo": {
    "name": "UGoing",
    "slug": "UGoing - The world's fastest event scheduler",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "image": "./src/assets/UGoing-Link-Preview.png",
    "description": "The worldâ€™s fastest event scheduler",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "Allow $(PRODUCT_NAME) to access your calendar",
        "NSRemindersUsageDescription": "Allow $(PRODUCT_NAME) to access your reminders"
      },
      "bundleIdentifier": "ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "css"
      ]
    }
  }
}
